    <rdfs:label>the context of a decision in case law, corresponds to caselawext:context_decision in IMMC</rdfs:label>
  </owl:AnnotationProperty>

  <owl:AnnotationProperty rdf:about="&annotation;context">
    <rdfs:label>the context in which a statement holds true, e.g. for the implementation of certain directives into national law</rdfs:label>
  </owl:AnnotationProperty>

  <owl:AnnotationProperty rdf:about="&annotation;status">
    <rdfs:label>status of an identifier manifestation, typically "valid" (if ever explicitly stated) or "invalid" (primarily for General Publications)</rdfs:label>
  </owl:AnnotationProperty>

  <owl:AnnotationProperty rdf:about="&annotation;changed_to_identifier">
    <rdfs:label>A wrongly built identifier has been changed to another one that is given here</rdfs:label>
  </owl:AnnotationProperty>


  <owl:AnnotationProperty rdf:about="&annotation;ignore_identifier">
    <rdfs:label>The given identifier should be ignored, e.g. because it is erroneous</rdfs:label>
  </owl:AnnotationProperty>


  <owl:AnnotationProperty rdf:about="&annotation;to_be_indexed">
    <rdfs:label>A property with this annotation in the ontology must be included in index notices</rdfs:label>
  </owl:AnnotationProperty>


 <owl:AnnotationProperty rdf:about="&annotation;do_not_index">
    <rdfs:label>For any digital object carrying this annotation no index notice must be created</rdfs:label>
  </owl:AnnotationProperty>

  <owl:AnnotationProperty rdf:about="&annotation;read_only">
    <rdfs:label>For any digital object carrying this annotation the update should be rejected</rdfs:label>
  </owl:AnnotationProperty>
  
