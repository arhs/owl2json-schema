  <!-- Properties for sequencing, cf. https://webgate.ec.europa.eu/publications/jira/browse/METACONV-343 -->
  <owl:ObjectProperty rdf:about="&cdm;memberList">
    <rdfs:comment>Object property indicating on which providing th rdf:List that organizes a set of property values</rdfs:comment>
    <rdfs:label>Sequence of values</rdfs:label>
    <rdfs:range rdf:resource="&rdf;List"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="&cdm;listedProperty">
    <rdfs:comment>Object property identifying the property from which object values are listed</rdfs:comment>
    <rdfs:label>Property with list as range</rdfs:label>
    <rdfs:domain rdf:resource="&rdf;List"/>
    <rdfs:range rdf:resource="&rdf;Property"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="&cdm;nestedList">
    <rdfs:comment>Object property that is a sub-property of
    rdf:first. It introduces a nested list (as the first resource) at
    the current position in the rdf:List</rdfs:comment>
    <rdfs:label>Property to hold nested lists</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="&rdf;first"/>
    <rdfs:range rdf:resource="&rdf;List"/>
  </owl:ObjectProperty>
</rdf:RDF>
