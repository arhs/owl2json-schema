  <owl:AnnotationProperty rdf:about="&annotation;type_of_link_target">
    <rdfs:label>Type of the link symbolized by this link target</rdfs:label>
    <rdfs:comment>Value EA-SP to indicate that this relation is from the same procedure and should be displayed in Earlier (Subsequent) instruments on NEL</rdfs:comment>
  </owl:AnnotationProperty>
  <owl:AnnotationProperty rdf:about="&annotation;reference_to_modifying_location">
    <rdfs:label>Indication of the location which has been changed</rdfs:label>
    <rdfs:comment>Futher indication of the location which has been changed (e.g. 
    <![CDATA[<PLACE_A>M</PLACE_A>]]>)</rdfs:comment>
  </owl:AnnotationProperty>
  <owl:AnnotationProperty rdf:about="&annotation;reference_to_modified_location">
    <rdfs:label>Indication of the location which has been changed</rdfs:label>
    <rdfs:comment>Futher indication of the location which has been changed (e.g. 
    <![CDATA[<PLACE_P>PROT3 ANN 2</PLACE_P><DD_EV>1974/01/01</DD_EV>]]>)</rdfs:comment>
  </owl:AnnotationProperty>
  <owl:AnnotationProperty rdf:about="&annotation;role2">
    <rdfs:label>Description of the target role for the amended act</rdfs:label>
  </owl:AnnotationProperty>
  <owl:AnnotationProperty rdf:about="&annotation;start_of_validity">
    <rdfs:label>Start of validity</rdfs:label>
  </owl:AnnotationProperty>
  <owl:AnnotationProperty rdf:about="&annotation;end_of_validity">
    <rdfs:label>End of validity</rdfs:label>
  </owl:AnnotationProperty>
  <owl:AnnotationProperty rdf:about="&annotation;famille">
    <rdfs:label>It is at present unknown what the semantics of the contents of this field are</rdfs:label>
  </owl:AnnotationProperty>
  <owl:AnnotationProperty rdf:about="&annotation;language_list">
    <rdfs:label>List of language(s) concerned by this link</rdfs:label>
  </owl:AnnotationProperty>
  <owl:AnnotationProperty rdf:about="&annotation;approximation">
    <rdfs:label>Comment on additional information not precisely known</rdfs:label>
    <rdfs:comment>comment that indicates for the human reader some additional information that may be available on a  date that is not precisely known (e.g. "this publication was printed in the 1960s")</rdfs:comment>
  </owl:AnnotationProperty>
  <owl:AnnotationProperty rdf:about="&annotation;type_of_reply">
    <rdfs:label>Type of the reply to the question (makes only sense as the annotation of a date of reply)</rdfs:label>
  </owl:AnnotationProperty>
  <owl:AnnotationProperty rdf:about="&annotation;type_of_date">
    <rdfs:label>Type of the date (e.g. for IF_TYPE)</rdfs:label>
  </owl:AnnotationProperty>
  <owl:AnnotationProperty rdf:about="&annotation;comment_on_date">
    <rdfs:label>Comment on the date (e.g. for IF_COMMENT, LB_DN_COMMENT)</rdfs:label>
  </owl:AnnotationProperty>
  <owl:AnnotationProperty rdf:about="&annotation;in_committee">
    <rdfs:label>An agent is part of a parliamentary committee</rdfs:label>
  </owl:AnnotationProperty>
  <owl:AnnotationProperty rdf:about="&annotation;transposition_deadline_transmitted">
    <rdfs:label>Transposition deadline that the SecGen indicates for a NIM</rdfs:label>
  </owl:AnnotationProperty>
  <owl:AnnotationProperty rdf:about="&annotation;transposition_notification">
    <rdfs:label>Notication date by the Member State</rdfs:label>
  </owl:AnnotationProperty>
  <owl:AnnotationProperty rdf:about="&annotation;result_decision">
    <rdfs:label>Result of the decision of a judicial procedure, encoded as http://publications.europa.eu/mdr/resource/authority/procresult/html/procresult-eng.html</rdfs:label>
  </owl:AnnotationProperty>
  <owl:AnnotationProperty rdf:about="&annotation;eli_source">
    <rdfs:label>ELI of the source work of this relationship. Can be either the ELI of a complete act or the ELI of a subdivision</rdfs:label>
  </owl:AnnotationProperty>
  <owl:AnnotationProperty rdf:about="&annotation;eli_target">
    <rdfs:label>ELI of the target work of this relationship. Can be either the ELI of a complete act or the ELI of a subdivision</rdfs:label>
  </owl:AnnotationProperty>

